{
  "build": {
    "content": [
      {
        "files": [
          "stream/**.md",
          "stream/**.yml",
          "artist/**.md",
          "artist/**.yml",
          "ci/**.md",
          "ci/**.yml",
          "cloud/**.md",
          "cloud/**.yml",
          "dev/**.md",
          "dev/**.yml",
          "unity/**.md",
          "unity/**.yml",
          "toc.yml",
          "*.md",
          "api/**.md",
          "api/**.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "metadata":[
      {
        "src": [
          {
            "src": "../Funique",
            "files": ["**.csproj"],
            "exclude": [
              "**/bin/**",
              "**/obj/**",
              "**/Properties/**"
            ]
          }
        ],
        "dest": "api",
		"filter": "filter.yml"
      }
    ],
    "dest": "../static",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appName": "Funique",
      "_appTitle": "Funique Documentation",
      "_enableSearch": true,
      "_disableAffix": true,
      "_appFooter": "Funique Inc.<br/>Generated by <b>DocFX</b>",
      "_disableSideFilter": true,
      "_gitContribute": {
        "repo": "https://github.com/Funique2022/.github"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": true,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}